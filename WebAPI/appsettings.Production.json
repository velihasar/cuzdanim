{
  "Services": {

  },
  "ConnectionStrings": {
    "DArchPgContext": "Host=${DB_HOST};Port=${DB_PORT};Database=${DB_NAME};Username=${DB_USER};Password=${DB_PASSWORD}",
    "DArchMsContext": "data source=${DB_HOST};initial catalog=${DB_NAME};persist security info=False;user id=${DB_USER};password=${DB_PASSWORD};",
    "DArchOracleContext": "Data Source=${DB_HOST}:1521;User Id=${DB_USER};Password=${DB_PASSWORD};",
    "DArchMySqlContext": "server=${DB_HOST}; port=3306; database=${DB_NAME}; user=${DB_USER}; password=${DB_PASSWORD}; Persist Security Info=False; Connect Timeout=300",
    "HangfireConnection": "Host=${DB_HOST};Port=${DB_PORT};Database=${HANGFIRE_DB_NAME};Username=${DB_USER};Password=${DB_PASSWORD}"
  },
  "TokenOptions": {
    "Audience": "${JWT_AUDIENCE}",
    "Issuer": "${JWT_ISSUER}",
    "AccessTokenExpiration": 10,
    "SecurityKey": "${JWT_SECURITY_KEY}"
  },
  "Turnstile": {
    "SiteKey": "${TURNSTILE_SITE_KEY}",
    "SecretKey": "${TURNSTILE_SECRET_KEY}"
  },
  "EmailConfiguration": {
    "SmtpServer": "${SMTP_SERVER}",
    "SmtpPort": "${SMTP_PORT}",
    "SenderName": "${SMTP_SENDER_NAME}",
    "SenderEmail": "${SMTP_SENDER_EMAIL}",
    "UserName": "${SMTP_USERNAME}",
    "Password": "${SMTP_PASSWORD}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AppSettings": {
    "Mode": "Production",
    "BaseUrl": "${BASE_URL}",
    "FrontendUrl": "${FRONTEND_URL}"
  },
  "WhiteList": [
    "::1"
  ],
  "AllowedHosts": "*",
  "MessageBrokerOptions": {
    "HostName": "${RABBITMQ_HOST}",
    "UserName": "${RABBITMQ_USERNAME}",
    "Password": "${RABBITMQ_PASSWORD}"
  },
  "CacheOptions": {
    "Host": "${REDIS_HOST}",
    "Port": "${REDIS_PORT}",
    "Password": "${REDIS_PASSWORD}",
    "Database": 0
  },
  "TaskSchedulerOptions": {
    "Enabled": true,
    "StorageType": "postgresql",
    "ConnectionString": "${HANGFIRE_CONNECTION_STRING}",
    "RecurringJobsJsonFilePath": "RecurringJobs.json",
    "Title": "Cuzdanim Scheduled Jobs",
    "Path": "/hangfire",
    "Username": "${HANGFIRE_USERNAME}",
    "Password": "${HANGFIRE_PASSWORD}"
  },
  "ElasticSearchConfig": {
    "ConnectionString": "${ELASTICSEARCH_URL}",
    "UserName": "${ELASTICSEARCH_USERNAME}",
    "Password": "${ELASTICSEARCH_PASSWORD}"
  },
  "MongoDbSettings": {
    "ConnectionString": "${MONGODB_CONNECTIONSTRING}",
    "DatabaseName": "${MONGODB_DATABASE}"
  },
  "SeriLogConfigurations": {
    "PostgreConfiguration": {
      "ConnectionString": "Host=${DB_HOST};Port=${DB_PORT};Database=${DB_NAME};Username=${DB_USER};Password=${DB_PASSWORD};"
    },
    "MsSqlConfiguration": {
      "ConnectionString": "data source=${DB_HOST};initial catalog=${DB_NAME};persist security info=False;user id=${DB_USER};password=${DB_PASSWORD};"
    },
    "OracleConfiguration": {
      "ConnectionString": "Data Source=${DB_HOST}:1521;User Id=${DB_USER};Password=${DB_PASSWORD};"
    },
    "FileLogConfiguration": {
      "FolderPath": "/logs/"
    },
    "MSTeamsConfiguration": {
      "ChannelHookAdress": "${TEAMS_WEBHOOK_URL}"
    },
    "MongoDbConfiguration": {
      "ConnectionString": "${MONGODB_CONNECTIONSTRING}",
      "Collection": "logs"
    }
  },
  "EmailEncryption": {
    "Key": "${EMAIL_ENCRYPTION_KEY}"
  },
  "Google": {
    "ClientId": "${GOOGLE_CLIENT_ID}"
  },
  "AdminSettings": {
    "Email": "${ADMIN_EMAIL}",
    "UserName": "${ADMIN_USERNAME}",
    "Password": "${ADMIN_PASSWORD}",
    "FullName": "${ADMIN_FULL_NAME}"
  }
}
